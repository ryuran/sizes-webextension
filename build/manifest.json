{
  "description": "Adds red outline on img if sizes attribute is wrong for the current viewport size",
  "manifest_version": 2,
  "name": "Sizes",
  "version": "1.1.0",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_title": "Sizes"
  }
}
